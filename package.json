{
  "name": "pipedrive-bling-interface",
  "version": "1.0.0",
  "description": "An interface App that sends the Pipedrive archived leads and place as  Bling new order",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/shared/infra/http/server.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist && copyfiles -u 1 src/**/*.html dist",
    "start": "node dist/shared/infra/http/server.js",
    "test": "jest --runInBand",
    "lint": "eslint src --ext ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mbressan06/pipdrive-bling-interface.git"
  },
  "keywords": [
    "pipedrive",
    "bling",
    "interface",
    "api",
    "restful"
  ],
  "author": "Marcelo Bressan",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mbressan06/pipdrive-bling-interface/issues"
  },
  "engines": {
    "node": ">=12.3.1"
  },
  "dependencies": {
    "@sentry/node": "^5.20.1",
    "bcryptjs": "^2.4.3",
    "bull": "^3.16.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.9.24",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.3.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "1.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.14.0",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.7",
    "@types/jest": "26.0.4",
    "@types/jsonwebtoken": "8.5.0",
    "@types/mongoose": "5.7.32",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.1.0",
    "mongodb": "3.5.9",
    "prettier": "2.0.5",
    "ts-jest": "26.1.3",
    "ts-node": "8.10.2",
    "ts-node-dev": "1.0.0-pre.52",
    "tsconfig-paths": "3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "3.9.7"
  }
}
